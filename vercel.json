{
    "version": 2,  
    "builds": [
      {
        "src": "manage.py",  
        "use": "@vercel/python",  
        "config": { "maxLambdaSize": "50mb" }  
      },
      {
        "src": "customer/package.json",  
        "use": "@vercel/static-build",  
        "config": { "distDir": "build" }  
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",  
        "dest": "/static/$1"  
      },
      {
        "src": "/media/(.*)",  
        "dest": "/media/$1"  
      },
      {
        "src": "/(.*)",  
        "dest": "manage.py"  
      }
    ],
    "env": {
      "DJANGO_SETTINGS_MODULE": "EDteam.settings",  
      "PYTHONUNBUFFERED": "1"  
    },
